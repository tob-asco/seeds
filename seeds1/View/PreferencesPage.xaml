<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:seeds1.ViewModel"
             xmlns:mauiModel="clr-namespace:seeds1.MauiModels"
             x:Class="seeds1.View.PreferencesPage"
             x:DataType="local:PreferencesViewModel"
             Title="Preferences">
    <CollectionView
        ItemsSource="{Binding CatPreferences}"
        >
        <CollectionView.ItemTemplate>
            <DataTemplate x:DataType="mauiModel:CatPreference">
                <Button 
                        Text="{Binding Name}"
                        >
                </Button>
            </DataTemplate>
        </CollectionView.ItemTemplate>
    </CollectionView>
</ContentPage>